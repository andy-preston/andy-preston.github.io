<body webc:nokeep>
    <article-header
        webc:nokeep
        webc:if="tags"
        :title="title"
        :date="date"></article-header>
    <article
        webc:nokeep
        @raw="content"></article>
    <hr>
    <article webc:if="page.url != '/'">
        <a href="/">More stuff to read</a>
    </article>
</body>
