{
    "compilerOptions": {
        "lib": ["ES2015"],
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "noEmit": true,
        "allowImportingTsExtensions": true
    }
}
